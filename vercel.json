{
  "version": 2,
  "name": "nordiqflow",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/investors",
      "dest": "/investors.html"
    },
    {
      "src": "/af-issues",
      "dest": "/af-issues.html"
    },
    {
      "src": "/tech",
      "dest": "/tech.html"
    },
    {
      "src": "/cityiq",
      "dest": "/cityiq.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
