{
  "version": 2,
  "name": "nordiqflow",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "data/processed/**/*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/investors",
      "dest": "/investors.html"
    },
    {
      "src": "/af-data",
      "dest": "/af-data.html"
    },
    {
      "src": "/af-issues",
      "dest": "/af-issues.html"
    },
    {
      "src": "/tech",
      "dest": "/tech.html"
    },
    {
      "src": "/cityiq",
      "dest": "/cityiq.html"
    },
    {
      "src": "/algoritmen",
      "dest": "/algoritmen.html"
    },
    {
      "src": "/research",
      "dest": "/research.html"
    },
    {
      "src": "/demo",
      "dest": "/demo.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
